{
    "routes": [
        {
            "route": "/login",
            "allowedRoles": [
                "anonymous"
            ],
            "rewrite": "/.auth/login/b2c"
        },
        {
            "route": "/logout",
            "allowedRoles": [
                "authenticated"
            ],
            "rewrite": "/.auth/logout"
        },
        {
            "route": "/.auth/login/aad",
            "allowedRoles": [
                "anonymous",
                "authenticated"
            ],
            "statusCode": 404
        },
        {
            "route": "/.auth/login/b2c",
            "allowedRoles": [
                "anonymous"
            ]
        },
        {
            "route": "/.auth/login/google",
            "allowedRoles": [
                "anonyous",
                "authenticated"
            ],
            "statusCode": 404
        },
        {
            "route": "/dashboard",
            "allowedRoles": [
                "authenticated"
            ]
        }
    ],
    "responseOverrides": {
        "401": {
            "statusCode": 302,
            "redirect": "/login"
        }
    },
    "auth": {
        "identityProviders": {
            "customOpenIdConnectProviders": {
                "b2c": {
                    "registration": {
                        "clientIdSettingName": "AZURE_CLIENT_ID",
                        "clientCredential": {
                            "clientSecretSettingName": "AZURE_CLIENT_SECRET_APP_SETTING_NAME"
                        },
                        "openIdConnectConfiguration": {
                            "wellKnownOpenIdConfiguration": "https://digitaltrailsuva.b2clogin.com/digitaltrailsuva.onmicrosoft.com/B2C_1_SignUpSignIn/v2.0/.well-known/openid-configuration"
                        }
                    },
                    "login": {
                        "nameClaimType": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/name",
                        "scopes": ["https://digitaltrailsuva.onmicrosoft.com/api/read"],
                        "loginParameterNames": []
                    }
                }
            }
        }
    }
}